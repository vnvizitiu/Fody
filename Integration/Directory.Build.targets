<Project>

  <!--These properties are in Directory.Build.targets because the Integration solution uses a Directory.Build.props file that
  is generated by the main solution to provide the $(IntegrationVersion) package version property. -->
  <PropertyGroup>
    <LangVersion>latest</LangVersion>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>

    <!-- Fody warnings are expected in the integration test -->
    <FodyTreatWarningsAsErrors>false</FodyTreatWarningsAsErrors>
  </PropertyGroup>

</Project>
